{
    "name": "katsana/minions-polyfill",
    "description": "Minions Component Server (Polyfill) using Laravel Routing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@katsana.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Minions\\Polyfill\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minions\\Polyfill\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/http": "^6.0 || ^7.0",
        "nyholm/psr7": "^1.2",
        "symfony/psr-http-message-bridge": "^1.3 || ^2.0"
    },
    "require-dev": {
        "clue/block-react": "^1.3",
        "clue/buzz-react": "^2.6",
        "katsana/minions": "^1.2 || ^2.0",
        "orchestra/canvas": "^4.5 || ^5.0",
        "orchestra/testbench-dusk": "^4.6 || ^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Minions\\Polyfill\\MinionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
