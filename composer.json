{
    "name": "katsana/minions-polyfill",
    "description": "Laravel Routing Polyfill for Minions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Minions\\Polyfill\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minions\\Polyfill\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/http": "^6.0",
        "nyholm/psr7": "^1.2",
        "symfony/psr-http-message-bridge": "^1.2"
    },
    "require-dev": {
        "clue/buzz-react": "^2.5",
        "clue/block-react": "^1.3",
        "katsana/minions": "^1.2",
        "orchestra/canvas": "^4.5",
        "orchestra/testbench-dusk": "^4.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minions\\Polyfill\\PolyfillServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
